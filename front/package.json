{
    "name": "fe4newer",
    "version": "0.1.0",
    "scripts": {
        "postinstall": "gulp transpile",
        "debug": "babel-node-debug ./bin/www",
        "dev": "nodemon --exec babel-node ./bin/www -d 1 --ignore public/ --ignore sessions/",
        "start": "node ./bin/www"
    },
    "dependencies": {
        "body-parser": "~1.17.1",
        "connect-multiparty": "^2.0.0",
        "cookie-parser": "~1.4.3",
        "debug": "~2.6.3",
        "express": "~4.15.2",
        "express-session": "^1.13.0",
        "jade": "~1.11.0",
        "moment": "^2.12.0",
        "morgan": "~1.8.1",
        "rest": "^1.3.1",
        "serve-favicon": "~2.4.2",
        "session-file-store": "^0.2.0",
        "underscore": "^1.8.3",
        "winston": "^2.2.0",
        "winston-daily-rotate-file": "^1.0.1"
    },
    "devDependencies": {
        "babel-cli": "^6.6.5",
        "babel-preset-es2015": "^6.6.0",
        "browser-sync": "^2.18.2",
        "del": "^2.2.2",
        "gulp": "^3.9.1",
        "gulp-angular-templatecache": "^1.8.0",
        "gulp-babel": "^6.1.2",
        "gulp-cached": "^1.1.0",
        "gulp-cssnano": "^2.1.1",
        "gulp-eslint": "^3.0.1",
        "gulp-file-assets": "^2.2.2",
        "gulp-if": "^2.0.0",
        "gulp-jade": "^1.1.0",
        "gulp-load-plugins": "^1.2.4",
        "gulp-load-subtasks": "^1.1.2",
        "gulp-ng-annotate": "^2.0.0",
        "gulp-ngdocs": "^0.3.0",
        "gulp-rev": "^7.0.0",
        "gulp-rev-collector": "^1.0.2",
        "gulp-rev-replace": "^0.4.3",
        "gulp-strip-debug": "^1.1.0",
        "gulp-uglify": "^1.5.3",
        "gulp-useref": "^3.0.8",
        "gulp-util": "^3.0.7",
        "nodemon": "^1.11.0",
        "run-sequence": "^1.1.5"
    },
    "projectConfig": {
        "client": {
            "css": {
                "src": "",
                "dist": "dist/public/css/"
            },
            "image": {
                "src": "public/img/**",
                "dist": "dist/public/img/"
            },
            "js": {
                "src": "public/source/**/*.js",
                "lintrc": "public/.eslintrc.js",
                "dist": "dist/public/js/",
                "deps": {
                    "src": "public/js/deps/**/*",
                    "dist": "dist/public/js/deps/",
                    "bundles": "views/+(shared)/*.html"
                },
                "develop": {
                    "dist": "public/js/"
                }
            },
            "template": {
                "src": "views/partial-html/**/*.jade",
                "dist": "public/source/85ido/"
            },
            "rev": {
                "dist": "dist/public/rev/"
            }
        },
        "server": {
            "js": {
                "src": "+(middlewares|helpers|routes|services|datas)/**/*.js",
                "dist": "dist/"
            },
            "views": {
                "src": [
                    "views/!(partial-html|shared)/**/*",
                    "views/*.jade"
                ],
                "dist": "dist/views/"
            }
        }
    }
}
